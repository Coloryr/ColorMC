<UserControl
    x:Class="ColorMC.Gui.UI.Views.Main.GameControl"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Width="100"
    d:DesignHeight="150"
    d:DesignWidth="100"
    mc:Ignorable="d">
    <Grid>
        <Rectangle
            Name="Rectangle_Select"
            Fill="#FFF4F4F5"
            IsVisible="False"
            Stroke="#FF5ABED6"
            StrokeThickness="2" />
        <StackPanel Margin="5">
            <Image
                Name="Image1"
                Source="resm:ColorMC.Gui.Resource.Icon.game.png"
                Stretch="UniformToFill" />
            <TextBlock
                Name="TextBlock1"
                HorizontalAlignment="Center"
                Text="Name"
                TextAlignment="Center"
                TextWrapping="Wrap" />
        </StackPanel>
    </Grid>

</UserControl>
