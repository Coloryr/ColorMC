<UserControl
    x:Class="ColorMC.Gui.UI.Views.FlyoutsControl"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Rectangle
            Fill="#FFF4F4F5"
            Stroke="#FF5ABED6"
            StrokeThickness="3" />
        <StackPanel Margin="5">
            <Button
                Width="80"
                Height="25"
                Content="启动实例" />
            <Button
                Width="80"
                Height="25"
                Content="查看Mod" />
            <Button
                Width="80"
                Height="25"
                Content="查看配置" />
            <Button
                Width="80"
                Height="25"
                Content="查看地图" />
            <Button
                Width="80"
                Height="25"
                Content="修改分组" />
        </StackPanel>
    </Grid>
</UserControl>
