<UserControl
    x:Class="ColorMC.Gui.UI.Controls.Hello.Tab5Control"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Rectangle Stroke="#FF5ABED6" StrokeThickness="3" />

        <StackPanel Margin="5">
            <Label
                Margin="5"
                HorizontalAlignment="Center"
                Content="最后确认"
                FontSize="40" />
            <StackPanel Margin="5" Orientation="Horizontal">
                <Label
                    VerticalAlignment="Center"
                    Content="目前导入Java数目："
                    FontSize="20" />
                <Label
                    Name="Label_Count1"
                    VerticalAlignment="Center"
                    Content="0"
                    FontSize="20" />
            </StackPanel>
            <StackPanel Margin="5" Orientation="Horizontal">
                <Label
                    VerticalAlignment="Center"
                    Content="目前登录账户数目："
                    FontSize="20" />
                <Label
                    Name="Label_Count2"
                    VerticalAlignment="Center"
                    Content="0"
                    FontSize="20" />
            </StackPanel>
            <StackPanel Margin="5" Orientation="Horizontal">
                <Label
                    VerticalAlignment="Center"
                    Content="目前游戏实例数目："
                    FontSize="20" />
                <Label
                    Name="Label_Count3"
                    VerticalAlignment="Center"
                    Content="0"
                    FontSize="20" />
            </StackPanel>

            <StackPanel Margin="5" Orientation="Horizontal">
                <Button
                    Name="Button_LoadConfig"
                    Width="100"
                    Height="25"
                    Margin="5"
                    Content="导入配置" />
                <Button
                    Name="Button_AddJava"
                    Width="100"
                    Height="25"
                    Margin="5"
                    Content="添加Java" />
                <Button
                    Name="Button_AddUser"
                    Width="100"
                    Height="25"
                    Margin="5"
                    Content="添加账户" />
                <Button
                    Name="Button_AddGame"
                    Width="100"
                    Height="25"
                    Margin="5"
                    Content="添加游戏" />
            </StackPanel>
        </StackPanel>

        <Button
            Name="Button_Next"
            Width="80"
            Height="25"
            Margin="0,0,10,10"
            HorizontalAlignment="Right"
            VerticalAlignment="Bottom"
            Content="完成" />
    </Grid>
</UserControl>
