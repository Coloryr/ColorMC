<Styles
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:setting="clr-namespace:ColorMC.Gui.Utils.LaunchSetting"
    xmlns:svg="clr-namespace:ColorMC.Gui.UI.Views.Svg">
    <Design.PreviewWith>
        <Border Padding="20">
            <svg:SvgControl Classes="t1" />
        </Border>
    </Design.PreviewWith>

    <Style Selector="svg|SvgControl">
        <Setter Property="Fill" Value="{setting:Colors Font}" />
        <Setter Property="Stroke" Value="{setting:Colors Font}" />
    </Style>
</Styles>