<UserControl
    x:Class="ColorMC.Gui.UI.Controls.Main.FlyoutsControl"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:setting="clr-namespace:ColorMC.Gui.Utils.LaunchSetting"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Rectangle
            Fill="{setting:Colors Back}"
            RadiusX="3"
            RadiusY="3"
            Stroke="{setting:Colors Main}"
            StrokeThickness="2" />
        <StackPanel Margin="3">
            <Button
                Name="Button1"
                Width="100"
                Height="25"
                Content="{setting:Localize MainWindow.Control.Text4}" />
            <Button
                Name="Button14"
                Width="100"
                Height="25"
                Content="{setting:Localize MainWindow.Flyouts.Text8}" />
            <Button
                Name="Button11"
                Width="100"
                Height="25"
                Content="{setting:Localize MainWindow.Flyouts.Text7}" />
            <Button
                Name="Button2"
                Width="100"
                Height="25"
                Content="{setting:Localize MainWindow.Flyouts.Text1}" />
            <Button
                Name="Button3"
                Width="100"
                Height="25"
                Content="{setting:Localize MainWindow.Flyouts.Text2}" />
            <Button
                Name="Button4"
                Width="100"
                Height="25"
                Content="{setting:Localize MainWindow.Flyouts.Text3}" />
            <Button
                Name="Button5"
                Width="100"
                Height="25"
                Content="{setting:Localize MainWindow.Flyouts.Text4}" />
            <Button
                Name="Button7"
                Width="100"
                Height="25"
                Content="{setting:Localize Button.OpFile}" />
            <Button
                Name="Button9"
                Width="100"
                Height="25"
                Content="{setting:Localize MainWindow.Control.Text8}" />
            <Button
                Name="Button8"
                Width="100"
                Height="25"
                Content="{setting:Localize MainWindow.Control.Text6}" />
            <Button
                Name="Button12"
                Width="100"
                Height="25"
                Content="{setting:Localize MainWindow.Control.Text12}" />
            <Button
                Name="Button6"
                Width="100"
                Height="25"
                Content="{setting:Localize MainWindow.Flyouts.Text5}" />
            <Button
                Name="Button13"
                Width="100"
                Height="25"
                Content="{setting:Localize MainWindow.Control.Text13}" />
            <Button
                Name="Button10"
                Width="100"
                Height="25"
                Content="{setting:Localize MainWindow.Flyouts.Text6}" />
        </StackPanel>
    </Grid>
</UserControl>
