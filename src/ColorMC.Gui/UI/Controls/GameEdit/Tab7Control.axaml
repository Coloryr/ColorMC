<UserControl
    x:Class="ColorMC.Gui.UI.Controls.GameEdit.Tab7Control"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:setting="clr-namespace:ColorMC.Gui.Utils.LaunchSetting"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Rectangle Stroke="{setting:Colors Main}" StrokeThickness="3" />
        <DockPanel>
            <StackPanel DockPanel.Dock="Top" Orientation="Horizontal">
                <CheckBox
                    Name="CheckBox1"
                    Margin="10,5,0,0"
                    Content="{setting:Localize GameEditWindow.Tab7.Text1}"
                    IsChecked="True" />
                <CheckBox
                    Name="CheckBox2"
                    Margin="10,5,0,0"
                    Content="{setting:Localize GameEditWindow.Tab7.Text2}"
                    IsChecked="True" />
            </StackPanel>
            <TextBox
                Name="TextBox1"
                Margin="5"
                IsReadOnly="True"
                ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                TextWrapping="Wrap" />
        </DockPanel>
    </Grid>
</UserControl>
