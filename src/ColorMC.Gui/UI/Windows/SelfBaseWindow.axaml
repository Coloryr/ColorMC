<Window
    x:Class="ColorMC.Gui.UI.Windows.SelfBaseWindow"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dialog="clr-namespace:DialogHostAvalonia;assembly=DialogHost.Avalonia"
    xmlns:info="clr-namespace:ColorMC.Gui.UI.Controls.Info"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:setting="clr-namespace:ColorMC.Gui.Utils.LaunchSetting"
    xmlns:views="clr-namespace:ColorMC.Gui.UI.Controls"
    xmlns:views1="clr-namespace:ColorMC.Gui.UI.Controls.User"
    Title="Title"
    Width="760"
    Height="450"
    MinWidth="315"
    MinHeight="450"
    d:DesignHeight="450"
    d:DesignWidth="760"
    Background="{setting:Colors TranBack}"
    ExtendClientAreaChromeHints="NoChrome"
    ExtendClientAreaTitleBarHeightHint="-1"
    ExtendClientAreaToDecorationsHint="True"
    FontFamily="{setting:Font}"
    SystemDecorations="BorderOnly"
    TransparencyBackgroundFallback="Transparent"
    mc:Ignorable="d">
    <DockPanel>
        <views:HeadControl Name="WinHead" DockPanel.Dock="Top" />
        <Panel Name="Grid1">
            <Image Name="Image_Back" Stretch="UniformToFill" />

            <dialog:DialogHost CloseOnClickAway="True">
                <Panel>
                    <dialog:DialogHost.DialogContent />
                    <Grid Name="MainControl" />

                    <info:Info1Control IsVisible="False" />
                    <info:Info4Control IsVisible="False" />
                    <info:Info6Control IsVisible="False" />
                    <info:Info5Control IsVisible="False" />
                    <info:Info3Control IsVisible="False" />
                    <info:Info2Control />

                    <Grid
                        Name="Grid2"
                        Background="{setting:Colors TopBottom}"
                        IsVisible="False">
                        <Label
                            Name="Label1"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Content="test"
                            FontSize="30" />
                    </Grid>
                </Panel>
            </dialog:DialogHost>
        </Panel>
    </DockPanel>
</Window>
